{
  "name": "aqua-stark-backend-api",
  "version": "1.0.0",
  "description": "Backend API for Aqua Stark - On-chain game based on Starknet",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "starknet",
    "dojo",
    "on-chain",
    "game",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.10.0"
  },
  "dependencies": {
    "@dojoengine/core": "1.8.5",
    "@fastify/multipart": "^8.3.1",
    "@supabase/supabase-js": "2.38.3",
    "@types/pg": "^8.15.6",
    "dotenv": "16.3.1",
    "fastify": "^4.29.1",
    "pg": "^8.16.3",
    "starknet": "5.14.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/ui": "^4.0.16",
    "tsx": "^4.7.0",
    "typescript": "5.3.3",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "ws": "^8.18.0"
  }
}
